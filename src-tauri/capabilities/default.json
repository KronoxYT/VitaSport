{
  "identifier": "default",
  "description": "Default capabilities for VitaSport",
  "windows": ["main"],
  "permissions": [
    {
      "identifier": "shell:open",
      "allow": [{"name": "open"}]
    },
    {
      "identifier": "fs:all",
      "allow": [{"name": "*"}]
    },
    {
      "identifier": "http:all",
      "allow": [{"name": "*"}]
    },
    {
      "identifier": "window:create",
      "allow": [{"name": "create"}]
    },
    {
      "identifier": "window:center",
      "allow": [{"name": "center"}]
    },
    {
      "identifier": "window:close",
      "allow": [{"name": "close"}]
    }
  ]
}
